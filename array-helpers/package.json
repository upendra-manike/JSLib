{
  "name": "@upendra.manike/array-helpers",
  "version": "1.0.2",
  "description": "Array manipulation utilities for JavaScript/TypeScript - group by, aggregate, statistics (average/median/mode), remove duplicates, and more. Comprehensive array operations for data processing and analysis.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "array",
    "group",
    "aggregate",
    "statistics",
    "average",
    "median",
    "javascript",
    "typescript",
    "array-utils",
    "array-helpers",
    "group-by",
    "count-by",
    "remove-duplicates",
    "array-statistics",
    "math",
    "array-operations",
    "array-functions",
    "data-processing",
    "array-manipulation",
    "array-library",
    "js-array",
    "ts-array",
    "array-tools",
    "statistics-library"
  ],
  "author": "Upendra Manike <upendra.manike@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
