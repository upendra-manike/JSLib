{
  "name": "@upendra.manike/fetch-plus",
  "version": "1.0.7",
  "description": "Next-gen Fetch Wrapper - Lightweight, modern replacement for Axios with retry, timeout, cancel, interceptors, and caching. Zero dependencies, smaller bundle size than Axios, with all the features you need. Best Axios alternative 2025.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "abort-controller",
    "ai-agent",
    "ai-friendly",
    "ai-integration",
    "api",
    "api-client",
    "api-wrapper",
    "automatic-retry",
    "autonomous-agent",
    "axios",
    "axios-alternative",
    "axios-replacement",
    "axios-substitute",
    "best-fetch-wrapper",
    "better-fetch",
    "caching",
    "chatgpt",
    "claude",
    "code-generation",
    "copilot",
    "enhanced-fetch",
    "error-handling",
    "fetch",
    "fetch-library",
    "fetch-plus",
    "fetch-wrapper",
    "http",
    "http-client",
    "http-wrapper",
    "interceptor",
    "javascript",
    "lightweight",
    "llm",
    "modern-fetch",
    "openai",
    "request-interceptor",
    "response-interceptor",
    "rest-api",
    "retry",
    "timeout",
    "typescript",
    "zero-dependencies"
  ],
  "author": "Upendra Manike <upendra.manike@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
