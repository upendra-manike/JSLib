{
  "name": "@upendra.manike/env-checker",
  "version": "1.0.2",
  "description": "Environment Variable Validator for JavaScript/TypeScript - Validate env vars against schema at startup. Prevent app crashes from missing configuration. Perfect for Node.js, React, Next.js, and all JavaScript applications.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "env",
    "environment",
    "validation",
    "config",
    "dotenv",
    "javascript",
    "typescript",
    "environment-variables",
    "env-vars",
    "config-validation",
    "startup-validation",
    "env-check",
    "dotenv-validation",
    "process-env",
    "config-check",
    "environment-check",
    "env-library",
    "config-library",
    "validation-tool"
  ],
  "author": "Upendra Manike <upendra.manike@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
