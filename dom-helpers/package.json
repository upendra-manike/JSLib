{
  "name": "@upendra.manike/dom-helpers",
  "version": "1.0.2",
  "description": "DOM and browser utilities for JavaScript/TypeScript - clipboard, viewport detection, smooth scroll, debounce/throttle. Essential browser utilities for modern web development. Perfect for performance optimization.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "dom",
    "clipboard",
    "viewport",
    "scroll",
    "debounce",
    "throttle",
    "javascript",
    "typescript",
    "browser",
    "dom-utils",
    "dom-helpers",
    "copy-to-clipboard",
    "clipboard-api",
    "viewport-detection",
    "lazy-loading",
    "smooth-scroll",
    "scroll-utils",
    "debounce-function",
    "throttle-function",
    "performance",
    "browser-utils",
    "web-utils",
    "dom-library",
    "js-dom",
    "ts-dom",
    "dom-tools"
  ],
  "author": "Upendra Manike <upendra.manike@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
