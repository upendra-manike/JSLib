{
  "name": "@upendra.manike/object-helpers",
  "version": "1.0.2",
  "description": "Object manipulation utilities for JavaScript/TypeScript - deep clone, deep merge, pick/omit, flatten nested objects. Comprehensive object operations with immutable patterns. Perfect for data transformation and manipulation.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "object",
    "clone",
    "deep-clone",
    "merge",
    "deep-merge",
    "pick",
    "omit",
    "deep-pick",
    "deep-omit",
    "flatten",
    "javascript",
    "typescript",
    "object-utils",
    "object-helpers",
    "object-manipulation",
    "object-operations",
    "deep-copy",
    "object-cloning",
    "object-merge",
    "immutable",
    "nested-objects",
    "object-library",
    "js-object",
    "ts-object",
    "object-tools"
  ],
  "author": "Upendra Manike <upendra.manike@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
