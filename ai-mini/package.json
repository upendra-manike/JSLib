{
  "name": "@upendra.manike/ai-mini",
  "version": "1.0.2",
  "description": "Universal LLM Client for JavaScript/TypeScript - Unified API for OpenAI, Gemini, Claude, Groq and more. One interface for all AI providers. Perfect for AI applications, chatbots, and LLM integrations. Switch between AI providers seamlessly.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "ai",
    "ai-agent",
    "ai-client",
    "ai-friendly",
    "ai-integration",
    "ai-provider",
    "ai-sdk",
    "ai-wrapper",
    "anthropic",
    "artificial-intelligence",
    "autonomous-agent",
    "chatgpt",
    "chatgpt-api",
    "claude",
    "claude-api",
    "code-generation",
    "copilot",
    "gemini",
    "google-gemini",
    "gpt",
    "gpt-api",
    "groq",
    "javascript-ai",
    "large-language-model",
    "llm",
    "llm-client",
    "llm-sdk",
    "multi-ai",
    "openai",
    "openai-api",
    "typescript-ai",
    "unified-ai"
  ],
  "author": "Upendra Manike <upendra.manike@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
