{
  "name": "@upendra.manike/id-generator",
  "version": "1.0.2",
  "description": "Secure ID generation for JavaScript/TypeScript - UUID v4, GUID, short unique strings, hash functions, and string masking utilities. Perfect for generating secure identifiers, tokens, and unique keys.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "uuid",
    "guid",
    "id",
    "hash",
    "unique",
    "javascript",
    "typescript",
    "uuid-generator",
    "guid-generator",
    "unique-id",
    "random-id",
    "secure-id",
    "id-generation",
    "hash-function",
    "mask",
    "data-masking",
    "string-hash",
    "id-library",
    "uuid-v4",
    "short-id",
    "random-string",
    "unique-string",
    "js-id",
    "ts-id"
  ],
  "author": "Upendra Manike <upendra.manike@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
