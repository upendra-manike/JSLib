{
  "name": "@upendra.manike/form-genie",
  "version": "1.0.2",
  "description": "Schema to Form generator for React - Build forms from JSON schema or Zod validation automatically. Eliminate repetitive form code. Auto-validation, conditional fields, and custom rendering. Perfect for admin panels, dashboards, and data entry forms. Best React form generator.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "ai-agent",
    "ai-friendly",
    "ai-integration",
    "auto-form",
    "auto-generate-forms",
    "auto-validation",
    "autonomous-agent",
    "chatgpt",
    "claude",
    "code-generation",
    "conditional-fields",
    "copilot",
    "dynamic-forms",
    "form",
    "form-builder",
    "form-components",
    "form-from-schema",
    "form-generator",
    "form-library",
    "formik-alternative",
    "formik-replacement",
    "input-validation",
    "json-schema",
    "llm",
    "openai",
    "react",
    "react-form-builder",
    "react-form-library",
    "react-forms",
    "react-hook-form-alternative",
    "react-hooks",
    "schema",
    "schema-to-form",
    "validation",
    "zod",
    "zod-validation"
  ],
  "author": "Upendra Manike <upendra.manike@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
