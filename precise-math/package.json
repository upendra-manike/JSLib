{
  "name": "@upendra.manike/precise-math",
  "version": "1.0.5",
  "description": "Decimal-safe math for JavaScript/TypeScript - accurate arithmetic (add, sub, mul, div, round) without floating-point errors. Plus factorial, prime numbers, fibonacci, GCD, LCM, number conversions, perfect numbers, Armstrong numbers, and more. Perfect for financial calculations, scientific computing, and AI agents.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "decimal",
    "math",
    "mathematics",
    "arithmetic",
    "floating-point",
    "precision",
    "arbitrary-precision",
    "accurate-math",
    "0.1+0.2",
    "financial",
    "money",
    "currency",
    "factorial",
    "prime",
    "prime-number",
    "fibonacci",
    "gcd",
    "lcm",
    "greatest-common-divisor",
    "least-common-multiple",
    "number-conversion",
    "binary",
    "decimal-to-binary",
    "binary-to-decimal",
    "perfect-number",
    "armstrong-number",
    "palindrome-number",
    "power-of-2",
    "temperature-conversion",
    "celsius",
    "fahrenheit",
    "interest-calculation",
    "compound-interest",
    "simple-interest",
    "number-to-words",
    "rounding",
    "round",
    "add",
    "subtract",
    "multiply",
    "divide",
    "javascript",
    "typescript",
    "nodejs",
    "browser",
    "ai-agent",
    "ai-friendly",
    "llm",
    "autonomous-agent",
    "code-generation",
    "chatgpt",
    "claude",
    "copilot",
    "scientific-computing",
    "numerical-analysis",
    "production-ready",
    "enterprise",
    "lightweight",
    "zero-dependencies"
  ],
  "author": "Upendra Manike <upendra.manike@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
