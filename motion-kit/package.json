{
  "name": "@upendra.manike/motion-kit",
  "version": "1.0.2",
  "description": "Framework-agnostic animation library - Modern animation components that work with React, Vue, Svelte, and vanilla JS. Prebuilt animations: fade, slide, scale, bounce. Use with any framework or vanilla JavaScript. Lightweight and performant animation library.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "animation",
    "motion",
    "framework-agnostic",
    "vanilla-js",
    "css",
    "javascript",
    "typescript",
    "animations",
    "fade",
    "slide",
    "scale",
    "bounce",
    "transitions",
    "ui-animations",
    "web-animations",
    "css-animations",
    "javascript-animations",
    "react",
    "vue",
    "svelte",
    "universal",
    "cross-framework",
    "vanilla-javascript",
    "pure-js",
    "animation-library",
    "js-animations",
    "browser-animations"
  ],
  "author": "Upendra Manike <upendra.manike@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
