{
  "name": "@upendra.manike/dev-utils",
  "version": "1.0.2",
  "description": "Developer utilities for JavaScript/TypeScript - logging with timestamps, execution time measurement, query string parser, and pub-sub event emitter. Essential tools for development and debugging.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "logging",
    "timing",
    "query",
    "parser",
    "event",
    "emitter",
    "javascript",
    "typescript",
    "logger",
    "performance",
    "measure-time",
    "execution-time",
    "query-string",
    "url-params",
    "event-emitter",
    "pub-sub",
    "publish-subscribe",
    "developer-tools",
    "dev-tools",
    "debugging",
    "performance-measurement",
    "url-parser",
    "query-params",
    "event-system",
    "js-utils",
    "ts-utils",
    "dev-utilities"
  ],
  "author": "Upendra Manike <upendra.manike@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
